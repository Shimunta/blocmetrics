<%= form_for (@registered_application) do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control', placeholder: "Name of App" %>
  </div>
  <div class="form-group">
    <%= f.label :URL %>
    <%= f.text_field :URL, class: 'form-control', placeholder: "URL of App" %>
  </div>
  <div class="form-group">
    <%= f.submit "Save", class: "btn btn-success" %>
  </div>
<% end %>
